<h1>Listing arrendatarios</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arrendatarios.each do |arrendatario| %>
      <tr>
        <td><%= arrendatario.Usuario %></td>
        <td><%= link_to 'Show', arrendatario %></td>
        <td><%= link_to 'Edit', edit_arrendatario_path(arrendatario) %></td>
        <td><%= link_to 'Destroy', arrendatario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Arrendatario', new_arrendatario_path %>
