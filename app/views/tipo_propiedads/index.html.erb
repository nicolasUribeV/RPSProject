<h1>Listing tipo_propiedads</h1>

<table>
  <thead>
    <tr>
      <th>Nombretipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_propiedads.each do |tipo_propiedad| %>
      <tr>
        <td><%= tipo_propiedad.NombreTipo %></td>
        <td><%= link_to 'Show', tipo_propiedad %></td>
        <td><%= link_to 'Edit', edit_tipo_propiedad_path(tipo_propiedad) %></td>
        <td><%= link_to 'Destroy', tipo_propiedad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo propiedad', new_tipo_propiedad_path %>
