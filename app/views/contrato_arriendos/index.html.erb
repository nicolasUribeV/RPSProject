<h1>Listing contrato_arriendos</h1>

<table>
  <thead>
    <tr>
      <th>Propiedad</th>
      <th>Arrendatario</th>
      <th>Fechainicioarriendo</th>
      <th>Fechaterminoarriendo</th>
      <th>Tiporenovacion</th>
      <th>Periodoreajuste</th>
      <th>Multaperiocidad</th>
      <th>Diapago</th>
      <th>Contratodigital</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contrato_arriendos.each do |contrato_arriendo| %>
      <tr>
        <td><%= contrato_arriendo.Propiedad %></td>
        <td><%= contrato_arriendo.Arrendatario %></td>
        <td><%= contrato_arriendo.FechaInicioArriendo %></td>
        <td><%= contrato_arriendo.FechaTerminoArriendo %></td>
        <td><%= contrato_arriendo.TipoRenovacion %></td>
        <td><%= contrato_arriendo.PeriodoReajuste %></td>
        <td><%= contrato_arriendo.MultaPeriocidad %></td>
        <td><%= contrato_arriendo.DiaPago %></td>
        <td><%= contrato_arriendo.ContratoDigital %></td>
        <td><%= link_to 'Show', contrato_arriendo %></td>
        <td><%= link_to 'Edit', edit_contrato_arriendo_path(contrato_arriendo) %></td>
        <td><%= link_to 'Destroy', contrato_arriendo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contrato arriendo', new_contrato_arriendo_path %>
