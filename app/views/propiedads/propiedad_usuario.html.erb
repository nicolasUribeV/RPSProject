<h1>Buscar Propiedades</h1>

<table>
  <thead>
    <tr>
      <th>Ubicacion</th>
      <th>Tipopropiedad</th>
      <th>Propietario</th>
      <th>Direccionpropiedad</th>
      <th>Habilitada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% Propiedad.joins("JOIN propietarios ON propietarios.id = propiedads.propietario_id").joins("JOIN usuarios ON propietarios.Usuario_id = usuarios.id").where("usuarios.Rut = ?",current_user.username).each do |propiedad| %>
      <tr>
        <td><%= propiedad.Ubicacion %></td>
        <td><%= propiedad.TipoPropiedad %></td>
        <td><%= propiedad.Propietario %></td>
        <td><%= propiedad.DireccionPropiedad %></td>
        <td><%= propiedad.Habilitada %></td>
        <td><%= link_to 'Show', propiedad %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>