<h1>Listing tipo_arriendos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Nombretipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_arriendos.each do |tipo_arriendo| %>
      <tr>
        <td><%= tipo_arriendo.Descripcion %></td>
        <td><%= tipo_arriendo.NombreTipo %></td>
        <td><%= link_to 'Show', tipo_arriendo %></td>
        <td><%= link_to 'Edit', edit_tipo_arriendo_path(tipo_arriendo) %></td>
        <td><%= link_to 'Destroy', tipo_arriendo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo arriendo', new_tipo_arriendo_path %>
