<div id="cuadro" style="background: #B0A9AA; margin-left: 30%; margin-right: 30%; border: solid 2px">
  <div class="row-fluid">
    <h1>Informacion de solicitud</h1>

    <div style="margin-left: 10px; margin-bottom: 20px">
      <p id="notice"><%= notice %></p>
      <p>
        <strong>Propiedad:</strong>
        <%= Propiedad.find(@solicitud.Propiedad).id %>
      </p>

      <p>
        <strong>Arrendatario:</strong>
        <%= Usuario.find(Arrendatario.find(@solicitud.Arrendatario).Usuario_id).Rut %>
      </p>

      <p>
        <strong>Fechaemision:</strong>
        <%= @solicitud.FechaEmision %>
      </p>

      <p>
        <strong>Estado:</strong>
        <%= @solicitud.Estado %>
      </p>

      <p>
        <strong>Tiposolicitud:</strong>
        <%= @solicitud.TipoSolicitud %>
      </p>

      <p>
        <strong>Inicio:</strong>
        <%= @solicitud.inicio %>
      </p>

      <p>
        <strong>Fin:</strong>
        <%= @solicitud.fin %>
      </p>
      <%= link_to 'Ir a Mis Solicitudes', "/solicituds/ver_solicitudes_cliente", :class => "btn btn-default" %>
    </div>
  </div>
</div>