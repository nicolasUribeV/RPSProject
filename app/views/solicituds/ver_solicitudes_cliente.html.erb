<h1>Mis solicitudes</h1>

<table>
  <thead>
    <tr>
      <th>Propiedad</th>
      <th>Fechaemision</th>
      <th>Estado</th>
      <th>Inicio</th>
      <th>Fin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicituds.each do |solicitud| %>
      <tr>
        <td><%= solicitud.Propiedad %></td>
        <td><%= solicitud.FechaEmision %></td>
        <td><%= solicitud.Estado %></td>
        <td><%= solicitud.inicio %></td>
        <td><%= solicitud.fin %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>