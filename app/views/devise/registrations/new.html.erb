<h2 style="color: black">Registrate</h2>
<div class="container">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<div class="formList">   
  <div><%= f.label :Rut %><%= f.text_field :username, autofocus: true %></div>
  <div><%= f.label :email %><%= f.email_field :email, autofocus: true %></div>
  <div><%= f.label :password %><%= f.password_field :password, autocomplete: "off" %></div>
  <div><%= f.label :password_confirmation %><%= f.password_field :password_confirmation, autocomplete: "off" %></div>
  <div><%= f.submit "Crear Usuario" %></div>
</div>   
<% end %>

<%= render "devise/shared/links" %>
</div>