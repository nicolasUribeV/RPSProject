<h1>Listing servicio_contratados</h1>

<table>
  <thead>
    <tr>
      <th>Servicio</th>
      <th>Contratopropiedad</th>
      <th>Precioacordado</th>
      <th>Periocidad</th>
      <th>Servicioactivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @servicio_contratados.each do |servicio_contratado| %>
      <tr>
        <td><%= servicio_contratado.Servicio %></td>
        <td><%= servicio_contratado.ContratoPropiedad %></td>
        <td><%= servicio_contratado.PrecioAcordado %></td>
        <td><%= servicio_contratado.Periocidad %></td>
        <td><%= servicio_contratado.ServicioActivo %></td>
        <td><%= link_to 'Show', servicio_contratado %></td>
        <td><%= link_to 'Edit', edit_servicio_contratado_path(servicio_contratado) %></td>
        <td><%= link_to 'Destroy', servicio_contratado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Servicio contratado', new_servicio_contratado_path %>
